{
    "extends": ["airbnb", "plugin:prettier/recommended", "prettier/react"],
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "jest": true,
        "node": true
    },
    "rules": {
        "prettier/prettier": "error",
        "no-console": "off",
        "react/jsx-filename-extension": "off",
        "react/prop-types": 0
    }
}
